<query id="getProjectLangLastUpdate" action="select">
	<tables>
	   <table name="translation_contents"/>
	</tables>
	<columns>
	   <column name="max(reg_date)" alias="last_update"/>
	</columns>
	<conditions>
		<condition operation="equal" column="translation_project_srl"  var="translation_project_srl"/>
		<condition operation="equal" column="lang"  var="lang" pipe="and"/>
	</conditions>      
</query>
