<query id="getFile" action="select">
    <tables>
        <table name="translation_files" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
		<condition operation="equal" column="translation_file_srl" var="translation_file_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
