<query id="getProjectTotalCount" action="select">
    <tables>
        <table name="translation_projects" />
    </tables>
    <columns>
		<column name="count(*)" alias="total_project_count" />
    </columns>
    <conditions>
		<condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
