<query id="deleteFile" action="delete">
    <tables>
        <table name="translation_files" />
    </tables>
    <conditions>
        <condition operation="equal" column="translation_project_srl" var="translation_project_srl" filter="number"/>
        <condition operation="equal" column="translation_file_srl" var="translation_file_srl" filter="number"/>
    </conditions>
</query>
